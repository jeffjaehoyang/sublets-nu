{
  "name": "sublets-nu-client",
  "homepage": "https://sublets-nu-new.herokuapp.com/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^5.15.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/pro-duotone-svg-icons": "^5.15.2",
    "@fortawesome/pro-light-svg-icons": "^5.15.2",
    "@fortawesome/pro-regular-svg-icons": "^5.15.2",
    "@fortawesome/pro-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "chokidar-cli": "^2.1.0",
    "formik": "^2.2.6",
    "framer-motion": "^3.2.2-rc.1",
    "google-map-react": "^2.1.9",
    "google-maps-react": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.3.1",
    "react-facebook-login": "^4.1.1",
    "react-ga": "^3.3.0",
    "react-google-login": "^5.2.2",
    "react-image-gallery": "^1.0.8",
    "react-images": "^1.1.7",
    "react-infinite-scroll-component": "^6.0.0",
    "react-loader-spinner": "^4.0.0",
    "react-modal": "^3.12.1",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-select": "^4.1.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.1",
    "tailwindcss": "^2.0.3",
    "twin.macro": "^2.2.0",
    "web-vitals": "^0.2.4",
    "yup": "^0.32.8"
  },
  "scripts": {
    "build:tailwind-dev": "postcss src/styles/tailwind.css -o src/styles/tailwind.output.css",
    "build:tailwind-prod": "cross-env postcss src/styles/tailwind.css -o src/styles/tailwind.generated.css",
    "watch:tailwind": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/styles/tailwind.output.css -c 'npm run build:tailwind'",
    "start": "npm-run-all build:tailwind-dev --parallel watch:tailwind start:react",
    "start:react": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babelMacros": {
    "twin": {
      "config": "./src/tailwind.config.js",
      "preset": "styled-components"
    }
  },
  "proxy": "http://localhost:8000",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.2",
    "autoprefixer": "^10.2.5",
    "cssnano": "^4.1.10",
    "postcss-cli": "^8.3.1"
  }
}
